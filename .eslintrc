{
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "env": {
    "browser": true
  },
  "extends": "standard",
  "rules": {
    "no-unused-vars": 1,
    "space-before-function-paren": 0,
    "comma-dangle": [1, "only-multiline"]
  },
  "globals": {
    "Tone": "readonly"
  }
}
